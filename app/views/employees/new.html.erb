<div>
	<%= form_for :employee do |f| %>
		<%= f.label :name %>
		<%= f.text_field :name %>
		<%= f.label :phone_number%>
		<%= f.telephone_field :phone_number %>
		<%= f.label :email %>
		<%= f.email_field :email %>
		<%= f.label :street_address %>
		<%= f.text_field :street_address %>
		<%= f.label :city %>
		<%= f.text_field :city %>
		<%= f.label :state %>
		<%= f.text_field :state %>
		<%= f.label :birth_date %>
		<%= f.text_field :birth_date %>
		<%= f.label :date_hired %>
		<%= f.text_field :date_hired %>
		<%= f.submit "Add Employee" %>
	<% end %>
</div>